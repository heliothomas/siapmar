	<subDataset name="dsLogin" uuid="db2ce432-d124-4d08-9c28-4cb196f16976">
		<queryString>
			<![CDATA[SELECT c_uname, c_passw, s_cargo, s_name, s_pemail, s_semail, case v_estado when 1 then 'activo' else 'inactivo' END as estado FROM login;]]>
		</queryString>
